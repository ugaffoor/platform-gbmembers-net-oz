      "events": [
        {
          "name": "Load Members Select",
          "type": "Load",
          "action": "Custom",
          "code": "bundle.termsInfo;\n  \nbundle.expandWaiverDetails = function(){\n    debugger;\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$studentName$\", K('field[Student First Name]').value()+\" \"+K('field[Student Last Name]').value());\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$legalName$\", K('space').attributes('School Legal Name'));\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$schoolName$\", K('space').attributes('School Name'));\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$address$\", K('space').attributes('School Address'));\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$acn$\", K('space').attributes('ACN'));\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$email$\", K('space').attributes('School Email'));\n    bundle.termsInfo=bundle.termsInfo.replaceAll(\"$billingCompany$\", K('space').attributes('Billing Company'));\n\t\tbundle.termsInfo=bundle.termsInfo.replaceAll(\"$state$\", K('space').attributes('School State'));\n\t\tbundle.termsInfo=bundle.termsInfo.replaceAll(\"$coolingPeriod$\", K('space').attributes('Cooling Period'));\n\t\tbundle.termsInfo=bundle.termsInfo.replaceAll(\"$online_individual_price$\", K('space').attributes('Online Week Individual Price'));\n\t\tbundle.termsInfo=bundle.termsInfo.replaceAll(\"$online_family_price$\", K('space').attributes('Online Week Family Price'));\n    \n    K('field[Waiver Information Text]').value(bundle.termsInfo);    \n    $(\"#waiverInformation\").html(bundle.termsInfo);    \n    \n  }\nbundle.GetURLParameter = function (sParam){\n    arr=window.location.hash.split(sParam+\"=\");\n    if (arr.length>0){\n      return arr[1];\n    }\n    return null;\n  }  \n  \nbundle.agreeTerms = function () {\n    K('field[Agreement]').value(\"Agreed\");\n    K('form').submitPage();\n  }\nbundle.noAgreeTerms = function() {\n    K('field[Agreement]').value(\"NOT Agreed\");\n    K('form').submitPage();\n  }\n\nK('bridgedResource[Members]').load({\n  attributes: ['Member ID', 'First Name', 'Last Name', 'Covid19 Waiver'],\n  success: function(bridgedData) {\n debugger;\n    var options=[];\n    $.each( bridgedData, function( key, value ) {\n/*      if (value['Covid19 Waiver']===null || value['Covid19 Waiver']===\"\" || value['Covid19 Waiver']===\"NOT Agreed\") { */\n\t      options[options.length]={'label':value['First Name']+\" \"+value['Last Name'],'value':value['id']};\n/*      }*/\n    });\n\t\tK('field[Members]').options(options);\n    \n    var label=null;\n\t\tK('field[Members]').options(options);\n    if (bundle.GetURLParameter(\"id\")!==null && bundle.GetURLParameter(\"id\")!==undefined) {\n    \tK('field[Members]').value(bundle.GetURLParameter(\"id\"));\n      $(K('field[Members]').options()).each(function(index) {\n       if (K('field[Members]').options()[index].value===bundle.GetURLParameter(\"id\")) label=K('field[Members]').options()[index].label;\n      });      \n    }\n    \n    bundle.config.widgets.selectMenu({\n      element: K(\"form\").find(\"#selectMemberMenu\")[0],\n      options: options,\n      onChange: function(option) {\n        K('field[Members]').value(option.value);\n        setTimeout(function(){\n          $(K(\"form\").find(\"#selectMemberMenu .css-1492t68\")).html(option.label);\n        },100);\n      },\n      value: K('field[Members]').value(),\n    });  \n    \n    if (label!==null){\n    \tK('section[Select Member]').hide();      \n    }    \n  }\n});\n",
